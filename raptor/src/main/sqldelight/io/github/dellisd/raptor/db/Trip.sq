import io.github.dellisd.kgtfs.domain.model.RouteId;
import io.github.dellisd.kgtfs.domain.model.TripId;

CREATE TABLE Trip (
    id TEXT AS TripId PRIMARY KEY,
    route_id TEXT AS RouteId NOT NULL,
    FOREIGN KEY (route_id) REFERENCES Route(id)
);

CREATE INDEX idx_trip_route_id ON Trip (route_id, id);

