import ca.derekellis.kgtfs.csv.AgencyId;

CREATE TABLE Agency (
    agency_id TEXT AS AgencyId,
    agency_name TEXT NOT NULL PRIMARY KEY,
    agency_url TEXT NOT NULL,
    agency_timezeone TEXT NOT NULL,
    agency_lang TEXT,
    agency_phone TEXT,
    agency_fare_url TEXT,
    agency_email TEXT
);

insert:
INSERT INTO Agency VALUES (?, ?, ?, ?, ?, ?, ?, ?);

getAll:
SELECT * FROM Agency;

getById:
SELECT * FROM Agency WHERE agency_id = ?;
